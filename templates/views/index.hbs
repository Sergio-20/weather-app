{{>header}}

  <h1>{{title}}</h1>
  
  <form id="query-form">
    <input id="search" type="text" placeholder="Los Angeles" />
    <button class="btn" type="submit">Submit</button>
  </form>

  <div class="search-results">
    <p class="address"></p>
    <h2 class="location"></h2>
    <h3>Forecast breakdown:</h3>
    <p class="forecast"></p>
  </div>

  <script type="application/javascript" src="/js/formQuery.js"></script>

{{>footer}}